#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
   int base,height,s1,s2,s3;
    scanf("%d\n %d\n %d\n %d\n %d",&base,&height,&s1,&s2,&s3);
    float s=(float)1/2*base*height;
    float a=(float)s1+s2+s3;
    printf("Area of Triangle is %.2f\n",s);
    printf("Perimeter of Triangle is %.2f",a);
    return 0;
}
